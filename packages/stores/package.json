{
  "name": "@prompt-master-pro/stores",
  "version": "2.0.0",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "zustand": "^4.4.0",
    "immer": "^10.0.0"
  },
  "devDependencies": {
    "eslint": "^8.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  }
}