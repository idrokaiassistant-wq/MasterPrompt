{
  "name": "master-prompt",
  "version": "2.0.0",
  "private": true,
  "description": "Professional prompt engineering platform with Telegram integration - Railway Pro Optimized",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "start:local": "turbo run start",
    "start:prod": "concurrently \"node scripts/monitor.js start\" \"npm run start --workspace=@prompt-master-pro/web\" \"node apps/api-gateway/dist/index.js\" \"node apps/telegram-service/dist/bot.js\"",
    "start:telegram": "node apps/telegram-service/dist/bot.js",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "clean": "turbo run clean && rm -rf node_modules",
    "deploy": "turbo run deploy",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "git:push": "node scripts/safe-push.js"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/node-fetch": "^2.6.13",
    "concurrently": "^9.2.1",
    "prisma": "^5.7.0",
    "tsx": "^4.6.0",
    "turbo": "^1.11.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "node-fetch": "^3.3.2",
    "redis": "^4.6.13"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.0.0"
}